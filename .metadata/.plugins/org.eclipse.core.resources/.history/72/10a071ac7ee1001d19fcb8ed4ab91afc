package ar.edu.unlam;

public class Usuario {
	
	private String nombreDeUsuario;
	private Boolean estaLogueado;
	private Boolean estaBlogueado;
	
	public Usuario(String nombreDeUsuario) {
		this.setNombreDeUsuario(nombreDeUsuario);
		this.setEstaLogueado(false);
		this.estaBlogueado = false;
	}

	public String getNombreDeUsuario() {
		return nombreDeUsuario;
	}

	public void setNombreDeUsuario(String nombreDeUsuario) {
		this.nombreDeUsuario = nombreDeUsuario;
	}

	public Boolean getEstaLogueado() {
		return estaLogueado;
	}

	public void setEstaLogueado(Boolean estaLogueado) {
		this.estaLogueado = estaLogueado;
	}

	public Boolean getEstaBlogueado() {
		return estaBlogueado;
	}

	public void setEstaBlogueado(Boolean estaBlogueado) {
		this.estaBlogueado = estaBlogueado;
	}
	
	
	

}
