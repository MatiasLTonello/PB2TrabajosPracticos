package ar.edu.unlam;

public class Equipo {

	  private Jugador[] jugadores = new Jugador[25];

	  public boolean agregarJugador(Jugador jugador) {
		    if (jugadores.length < 25) {
		        for (int i = 0; i < jugadores.length; i++) {
		            if (jugadores[i] == null) {
		                jugadores[i] = jugador;
		                return true;
		            }
		        }
		    }
		    return false;
		}

}
