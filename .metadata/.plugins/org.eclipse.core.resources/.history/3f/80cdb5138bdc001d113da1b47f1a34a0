			final int NOTA_DEL_PRIMER_PARCIAL = 8;
			final int NOTA_DEL_SEGUNDO_PARCIAL = 9;
			final int ASISTENCIA = 12;
			Alumno actual;
		// Ejecucion
		
		
		//Validacion
	}
	
}
